<?xml version="1.0"?>
<extension key="namelessevents" type="module">
  <file>namelessevents</file>
  <name>Nameless Network Participant Progress</name>
  <description>Support hiding of event online registration profiles based on the calculated contact sub-type of the participant.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/civicrm-namelessevents</url>
    <url desc="Documentation">https://github.com/twomice/civicrm-namelessevents</url>
    <url desc="Support">https://joineryhq.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <requires>
    <ext>namelessprogress</ext>
    <ext>com.joineryhq.profcond</ext>
  </requires>
  <releaseDate>2020-03-12</releaseDate>
  <version>1.0</version>
  <develStage>beta</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed for NamelessNetwork.org by Joinery</comments>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Namelessevents</namespace>
  </civix>
</extension>
